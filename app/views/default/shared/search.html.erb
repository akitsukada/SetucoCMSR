<!-- entry START -->
<div class="entry">
    <div class="entryHead">
        <h2><span>「<%= @query %>」の検索結果（<%= @total_count %>件）</span></h2>
    </div>
    <% if @search_result.count > 0 %>
    <%= render 'default/shared/search_result' %>
    <% else %>
    <%= render 'default/shared/search_result_not_found' %>
    <% end %>
</div>
<!-- entry END -->
<%= paginate @search_result %>
